    <style> 
      * {
        margin: 0;
        padding: 0;
        box-sizing: border-box;
        font-family: "montserrat";
      }
      
      html,
      body {
        height: 100%;
        background-color: black;
        width: 100%;
      }
      *::selection {
        background-color: #fff;
        color: #95c11e;
      }
      
      body::-webkit-scrollbar {
        display: block;
        width: 8px;
        background: #95c11e;
      }
      body::-webkit-scrollbar-thumb {
        background-color: #fff;
      
        border-radius: 50px;
      }
      body {
        overflow-x: hidden;
      }
      #cursor {
        height: 20px;
        width: 20px;
        background-color: #95c11e;
        border-radius: 50%;
        position: fixed;
        z-index: 99;
        transition: all linear 0.1s;
      }
      #cursor-blur {
        height: 500px;
        width: 500px;
        background-color: rgba(150, 193, 30, 0.3);
        border-radius: 50%;
        position: fixed;
        filter: blur(80px);
        z-index: 9;
        transition: all linear 0.4s;
      }
      #nav {
    height: 130px;
    width: 100%;
    background-color: black;
    display: flex;
    align-items: center;
    padding: 0 120px;
    gap: 50px;
    justify-content: flex-start;
    z-index: 999;
    color: white;
  }
  #nav img {
    height: 4.5vw;
  }
  #nav a{
    text-decoration: none;
    color: white;
  }
  #nav h4 {
    text-transform: uppercase;
    font-weight: 500;
    cursor: pointer;
    font-size: 1.15vw;
  }
  .link{
    display: flex;
    gap: 26px;
    padding-left: 15%;
  }
  .link a{
    text-align: center;
  }

  .page7{
  width: 100%;
  height: auto;
  background-color: #000;
}
.page7 .container{
  display: flex;
  justify-content: space-around;
  flex-wrap: wrap;
  gap: 5%;
}
.page7 h1{
  text-align: center;
  font-size: 10rem;
  color: white;
}
.page7 .card{
  display: flex;
  flex-direction: column;
  border-radius: 10px;
  height: 300px;
  width: 250px;
  margin-left: 30px;
  margin-right: 30px;
  margin-top: 60px;
  border-radius: 10px;
}
.page7 .card img{
  width: 250px;
  height: 200px ;
  border-top-left-radius: 10px;
  border-top-right-radius: 10px;
  background-size: cover;
  background-repeat: no-repeat;
}
.page7 .card .text{
  color: black;
  width: 250px;
  height: 100px;
  background-color: #F3F8FF;
  padding: 3px;
  border-bottom-left-radius: 10px;
  border-bottom-right-radius: 10px;
}
.page7 .card .text p,i{
  padding: 10px;
}
.page7 .card .text p{
  font-size: 1.4rem;
}

.page7 .card:hover{
  border: 2px solid greenyellow;
  
}

      #footer {
    height: 40vh;
    width: 100%;
    background: linear-gradient(to left bottom, #119f3a 0%, #a3d421 80%);
    position: relative;
    display: flex;
    align-items: center;
    justify-content: flex-start;
    gap: 6.5vw;
    padding: 0 100px;
    margin-top: 5%;
  }
  #footer > img {
    position: absolute;
    left: 0;
    height: 100%;
    z-index: 0;
  }
  #f1 img {
    height: 100px;
  }
  #f1,
  #f2,
  #f3,
  #f4 {
    width: fit-content;
    position: relative;
    z-index: 99;
    /* background-color: red; */
  }
  #f2 h3 {
    font-size: 1.6vw;
    white-space: nowrap;
    text-transform: uppercase;
    color: #000;
    font-weight: 900;
    margin-bottom: 8px;
  }
  
  #f3 h3 {
    font-size: 1.6vw;
    white-space: nowrap;
    text-transform: uppercase;
    color: #000;
    font-weight: 800;
    margin-bottom: 8px;
  }
  #f4 h4 {
    font-size: 1vw;
    white-space: nowrap;
    text-transform: uppercase;
    color: #000;
    font-weight: 600;
    line-height: 20px;
    margin-bottom: 8px;
  }

 
     
    </style>
     </head>
